---
import Layout from './Layout.astro';

interface Props {
  title: string;
}

const { title } = Astro.props;
---
<Layout title={title}>
  <main class="m-auto my-6 px-6 lg:px-0 w-full max-w-[1200px]">
		<div class="w-full h-0 md:h-12"></div>
		<div class="w-full">
      <slot />
    </div>
	</main>
</Layout>